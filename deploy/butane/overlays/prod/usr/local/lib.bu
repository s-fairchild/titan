---
variant: fcos
version: 1.5.0
storage:
  directories:
  - path: /usr/local/lib
    overwrite: true
  # Static helm charts must be uploaded separately from the ignition file.
  # the a helm chart tarball string is too long for json format.
  - path: /usr/local/lib/rancher/k3s/server/static/charts/extra

  trees:
  - path: /usr/local/lib/rancher/k3s/server/manifests
    local: butane/overlays/prod/usr/local/lib/rancher/k3s/server/manifests
